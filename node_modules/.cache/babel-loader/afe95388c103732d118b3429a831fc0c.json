ght: 100vw;\r\n    background-color: white;\r\n    position: fixed;\r\n    top: 0;\r\n    left: -105vw;\r\n    padding: 15px;\r\n    transition: left .5s;\r\n    z-index: 101;\r\n    overflow: scroll;\r\n\r\n    &.show {\r\n        left: 0;        \r\n    }\r\n}\r\n\r\n.drawer-close {\r\n    font-size: 1.5rem;\r\n    padding: 5px;\r\n    border-radius: 5px;\r\n    right: 10px;\r\n    border: 2px solid gray;\r\n    color: gray;\r\n    width: 15px;\r\n    float: right;\r\n    cursor: pointer;\r\n\r\n    &.hover {\r\n        background-color: lightgray;        \r\n    }\r\n}\r\n\r\n.data-details {\r\n    display: flex;\r\n    justify-content: center;\r\n    flex-direction: column;\r\n\r\n    p.description {\r\n        padding: 20px;\r\n        line-height: 1.5rem;\r\n    }\r\n\r\n.button-container {\r\n    button {\r\n        width: 150px;\r\n        border: none;\r\n        padding: 10px;\r\n        border-radius: 5px;\r\n        margin: 0 5px 50px 5px;\r\n        cursor: pointer;\r\n    } \r\n}\r\n}\r\n\r\n@media (min-width: 500px {\r\n    .drawer {\r\n        width: 450px;\r\n    }\r\n}\r\n</style>"]}]}