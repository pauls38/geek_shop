rn {};\n  },\n  methods: {},\n};\n</script>\n\n<style lang=\"scss\">\n@import url(\"https://fonts.googleapis.com/css2?family=Source+Sans+Pro&display=swap\");\n\n[data-tooltip]::after {\n  content: attr(data-tooltip);\n  position: absolute;\n  padding: 5px;\n  top: -30px;\n  left: -20px;\n  width: 70px;\n  color: #d8380e;\n  height: 20px;\n  font-size: 14px;\n  opacity: 0;\n  transition: 0.3s;\n  //border: 1px solid black;\n\n}\n\n.router-link:hover[data-tooltip]::after {\n  opacity: 1;\n}\n\n#header-app {\n  display: flex;\n  flex-direction: row;\n  justify-content: space-around;\n  align-items: center;\n  width: 100%;\n  min-height: 10px;\n  background: white;\n  //position: fixed;\n  //z-index: 99;\n  //border: 4px solid red;\n}\n.header-nav {\n  margin: 0 auto;\n  width: 100%;\n  //min-width: 600px;\n  min-height: 100px;\n  align-content: center;\n  align-items: center;\n  display: flex;\n  justify-content: space-around;\n  // border: 1px solid black;\n}\ninput[type=\"search\"]::-webkit-search-cancel-button {\n  display: none;\n}\n.router-link {\n  display: flex;\n  position: relative;\n  flex-direction: row;\n  justify-content: space-between;\n  text-decoration: none;\n  margin-left: 30px;\n  margin-top: 20px;\n  color: black;\n  font-family: \"Source Sans Pro\", sans-serif;\n  font-style: normal;\n  font-weight: 600;\n  height: 20px;\n  width: 20px;\n  //border: 1px solid black;\n  //font-size: 26px;\n  // background-color: #ffffff;\n  padding: 5px;\n  border-radius: 4px 4px;\n  //box-shadow: 0 2px 1px 1px black;\n  .icon-header-nav {\n    position: absolute;\n    top: 5px;\n    left: 0;\n    width: 30px;\n    height: 20px;\n    //transform: scale(1);\n    line-height: 20px;\n    //margin: 0 0 0 5px;\n    padding: 0;\n    border: none;\n  }\n  //.icon-header-nav:hover {\n  //  color: red;\n  //}\n}\n\n//.router-link:hover {\n//   background-color: #f1f4f3;\n//   box-shadow: 0 2px 1px 1px darkgray;\n//   color: #3f3f3f;\n//   transition: 0.5s;\n//}\n.router-link:hover .icon-header-nav {\n  color: #fc816c;\n  border: none;\n\n}\n\n//.router-link:active {\n//   box-shadow: 0 1px 0 1px gray;\n//   background-color: #e5e5e5;\n//   transition: 0.2s;\n//}\n\n.search {\n  -webkit-appearance: none;\n  -moz-appearance: none;\n  appearance: none;\n  background-color: #f5f5f5;\n  display: block;\n  appearance: none;\n  // width: 100%;\n  // height: 100%;\n  padding: 0.5rem 15.5rem 0.5rem 5.5rem;\n  margin-top: 20px;\n  margin-left: 10px;\n  margin-right: 0;\n  // margin: 0 auto;\n  line-height: 1.5;\n  border: 0;\n  border-radius: 50em;\n  outline: 0;\n  -webkit-box-shadow: none;\n  box-shadow: none;\n  -webkit-transition: background 0.2s;\n  -o-transition: background 0.2s;\n  transition: background 0.2s;\n}\n\n.img_logo {\n  width: 190px;\n  height: 80px;\n  //border: 1px solid black;\n  background-image: url(\"../assets/svalka.jpg\");\n  background-position: 40px 27px;\n  background-repeat: no-repeat;\n  background-size: contain;\n}\n@media (max-width: 1000px) {\n  .header-app {\n    flex-direction: row;\n    flex-wrap: wrap;\n    align-items: center;\n  }\n}\n@media (max-width: 980px) {\n  #header-app {\n    height: 230px;\n    align-items: flex-end;\n    flex-direction: column;\n  }\n  .search {\n    margin: 0 auto;\n  }\n  .img_logo {\n    //border: 1px solid black;\n    margin: 10px auto;\n    height: 50px;\n    line-height: 90px;\n    background-position: 40px -5px;\n\n  }\n  .header-nav {\n    width: 350px;\n    height: 160px;\n    flex-direction: row;\n    margin: 10px auto;\n  }\n\n  .router-link {\n    align-self: center;\n    margin: 5px 0 0 0;\n  }\n}\n//@media (max-width: 856px) {\n//\n//}\n//@media (max-width: 670px) {\n//  .router-link {\n//    align-self: center;\n//  }\n//}\n//@media (max-width: 500px) {\n\n//  .header-nav {\n//    flex-direction: row;\n//    height: 50px;\n//    min-width: 200px;\n//    margin: 0 0 0 35px;\n//  }\n//  .router-link {\n//    align-self: start;\n//    margin: 5px;\n//  }\n//}\n//@medi